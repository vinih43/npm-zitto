        "url": "git+https://github.com/igorskyflyer/{{repo}}.git"
    },
    "bugs": {
        "url": "https://github.com/igorskyflyer/{{repo}}/issues"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.2.4",
        "@igorskyflyer/biome-config": "^2.1.0",
        "@types/node": "^22.18.6",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
