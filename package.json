{
    "name": "@igorskyflyer/{{pkg}}",
    "version": "{{version}}",
    "description": "{{desc}}",
    "sideEffects": false,
    "engines": {
        "node": ">=22.0"
    },
    "type": "module",
    "types": "./dist/index.d.ts",
